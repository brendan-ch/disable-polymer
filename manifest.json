{
  "name": "Disable Polymer",
  "version": "1.0",
  "description": "Allows you to switch to the old classic YouTube layout.",
  "permissions": ["storage", "tabs", "webNavigation"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
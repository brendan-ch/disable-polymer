{
  "name": "Disable Polymer",
  "version": "1.0",
  "description": "Restores the old, pre-Material Design YouTube layout.",
  "permissions": ["storage", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}